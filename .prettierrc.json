{
  "tabWidth": 2,
  "singleQuote": false,
  "htmlWhitespaceSensitivity": "css",
  "jsxSingleQuote": false,
  "useTabs": false,
  "printWidth": 80,
  "bracketSameLine": false,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "semi": true,
  "quoteProps": "as-needed",
  "insertPragma": false,
  "proseWrap": "always"
}
